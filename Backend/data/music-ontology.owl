<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:music="http://example.org/music-ontology#">

    <owl:Ontology rdf:about="http://example.org/music-ontology">
        <rdfs:label>Music Ontology</rdfs:label>
        <rdfs:comment>Ontología extendida para modelar conceptos relacionados con música</rdfs:comment>
        <owl:versionInfo>2.0</owl:versionInfo>
    </owl:Ontology>

    <!-- CLASES -->
    <owl:Class rdf:about="http://example.org/music-ontology#Artist">
        <rdfs:label>Artista</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/music-ontology#Album">
        <rdfs:label>Álbum</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/music-ontology#Song">
        <rdfs:label>Canción</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/music-ontology#Instrument">
        <rdfs:label>Instrumento</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="http://example.org/music-ontology#Genre">
        <rdfs:label>Género</rdfs:label>
    </owl:Class>

    <!-- PROPIEDADES DE OBJETO -->
    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#hasAlbum">
        <rdfs:label>tiene álbum</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Album"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#containsSong">
        <rdfs:label>contiene canción</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Album"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Song"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#usesInstrument">
        <rdfs:label>usa instrumento</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Instrument"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#performedBy">
        <rdfs:label>interpretada por</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Artist"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#hasGenre">
        <rdfs:label>tiene género</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Album"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Genre"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#performsGenre">
        <rdfs:label>interpreta género</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Genre"/>
    </owl:ObjectProperty>

    <!-- PROPIEDADES DE DATOS -->
    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#name">
        <rdfs:label>nombre</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#description">
        <rdfs:label>descripción</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#duration">
        <rdfs:label>duración</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#releaseYear">
        <rdfs:label>año de lanzamiento</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#type">
        <rdfs:label>tipo</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- PROPIEDADES DE DATOS PARA ARTISTAS -->
    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#nationality">
        <rdfs:label>nacionalidad</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#trajectory">
        <rdfs:label>trayectoria</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#discography">
        <rdfs:label>discografía</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#birthYear">
        <rdfs:label>año de nacimiento</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#activeYears">
        <rdfs:label>años activo</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#awards">
        <rdfs:label>premios</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Artist"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- PROPIEDADES DE DATOS PARA CANCIONES -->
    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#language">
        <rdfs:label>idioma</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#composers">
        <rdfs:label>compositores</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#lyrics">
        <rdfs:label>letra</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://example.org/music-ontology#lyricist">
        <rdfs:label>letrista</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- PROPIEDADES DE OBJETO PARA COLABORACIONES -->
    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#collaboratesWith">
        <rdfs:label>colabora con</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Artist"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="http://example.org/music-ontology#featuringArtist">
        <rdfs:label>artista invitado</rdfs:label>
        <rdfs:domain rdf:resource="http://example.org/music-ontology#Song"/>
        <rdfs:range rdf:resource="http://example.org/music-ontology#Artist"/>
    </owl:ObjectProperty>

    <!-- GÉNEROS -->
    <music:Genre rdf:about="http://example.org/music-ontology#genre-rock">
        <music:name>Rock</music:name>
        <music:description>Género de música basado en guitarras eléctricas</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-pop">
        <music:name>Pop</music:name>
        <music:description>Género musical popular y accesible</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-jazz">
        <music:name>Jazz</music:name>
        <music:description>Género improvisado con raíces africanas</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-classical">
        <music:name>Clásica</music:name>
        <music:description>Música clásica europea orquestral</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-electronic">
        <music:name>Electrónica</music:name>
        <music:description>Género basado en sintetizadores</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-hiphop">
        <music:name>Hip-Hop</music:name>
        <music:description>Género urbano con rimas pegajosas</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-blues">
        <music:name>Blues</music:name>
        <music:description>Género emocional afroamericano</music:description>
    </music:Genre>
    <music:Genre rdf:about="http://example.org/music-ontology#genre-folk">
        <music:name>Folk</music:name>
        <music:description>Música tradicional de pueblos</music:description>
    </music:Genre>

