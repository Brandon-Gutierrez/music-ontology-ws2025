    <!-- ARTISTAS -->
    <music:Artist rdf:about="http://example.org/music-ontology#artist-john-lennon">
        <music:name>John Lennon</music:name>
        <music:description>Legendario miembro de The Beatles</music:description>
        <music:nationality>Reino Unido</music:nationality>
        <music:birthYear>1940</music:birthYear>
        <music:activeYears>1960-1980</music:activeYears>
        <music:trajectory>Cofundador de The Beatles, pionero del rock, compositor de canciones icónicas</music:trajectory>
        <music:discography>Please Please Me, With the Beatles, Abbey Road, Imagine, Double Fantasy</music:discography>
        <music:awards>Grammy Award, Inducción Rock and Roll Hall of Fame</music:awards>
        <music:performsGenre rdf:resource="http://example.org/music-ontology#genre-rock"/>
    </music:Artist>
    <music:Artist rdf:about="http://example.org/music-ontology#artist-paul-mccartney">
        <music:name>Paul McCartney</music:name>
        <music:description>Miembro fundador de The Beatles</music:description>
        <music:nationality>Reino Unido</music:nationality>
        <music:birthYear>1942</music:birthYear>
        <music:activeYears>1960-Presente</music:activeYears>
        <music:trajectory>Bajista, compositor y vocalista de The Beatles, solista exitoso</music:trajectory>
        <music:discography>Abbey Road, Wings Over America, Band on the Run, Ram</music:discography>
